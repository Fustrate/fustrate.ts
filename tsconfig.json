{
    "compilerOptions": {
        "declaration": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        // "lib": ["esnext"],
        "module": "commonjs",
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "outDir": "./dist/js",
        "plugins": [
            {
                "name": "typescript-tslint-plugin"
            }
        ],
        "strict": true,
        "target": "es6",
      },
      "exclude": ["node_modules", "**/test/*"],
      "include": ["src"],
  }
